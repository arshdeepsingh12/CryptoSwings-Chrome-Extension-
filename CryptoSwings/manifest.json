{
  "manifest_version": 2,	
  "name": "CryptoSwings",
  "version": "1.0",
  "description": "Check crypto prices on a click!!",
  "permissions": ["activeTab","https://bittrex.com/api/v1.1/public/getticker?market=USDT-BTC"],
  "browser_action": {                        //same as browser_action
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {"128":"icon_128.png"}
}
